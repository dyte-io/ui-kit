{
  "name": "@dytesdk/ui-kit",
  "version": "2.2.0-staging.2",
  "description": "Dyte's UI kit to support easy UI development over the core web library.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dyte-ui-kit/dyte-ui-kit.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "exports": {
    ".": {
      "import": "./dist/dyte-ui-kit/dyte-ui-kit.esm.js",
      "require": "./dist/dyte-ui-kit/dyte-ui-kit.cjs.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-in/ui-kit.git"
  },
  "bugs": {
    "url": "https://community.dyte.io"
  },
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec --watchAll",
    "test:coverage": "stencil test --spec --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,css,html,json}'",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "cp package.json package.json.bak && node prepublish.js",
    "postpublish": "mv package.json.bak package.json"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "peerDepdendencies": {
    "@dytesdk/web-core": ">=2.0.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.1.0",
    "hark": "^1.2.3",
    "hls.js": "^1.5.17",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@dyteinternals/utils": "^3.1.0",
    "@dytesdk/web-core": "staging",
    "@stencil-community/eslint-plugin": "^0.9.0",
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/angular-output-target": "^0.10.2",
    "@stencil/core": "4.25.1",
    "@stencil/react-output-target": "^0.8.2",
    "@stencil/store": "^2.0.16",
    "@stencil/vue-output-target": "^0.9.2",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/hark": "^1.2.2",
    "@types/jest": "^29.5.6",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.18.11",
    "autoprefixer": "^10.4.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tailwindcss": "3.1.0"
  },
  "license": "UNLICENSED"
}
