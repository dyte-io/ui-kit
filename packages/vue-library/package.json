    "postpublish": "mv package.json.bak package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-in/ui-kit.git"
  },
  "dependencies": {
    "@dytesdk/ui-kit": "*"
  },
  "bugs": {
    "url": "https://github.com/dyte-in/ui-kit/issues"
  }
}
